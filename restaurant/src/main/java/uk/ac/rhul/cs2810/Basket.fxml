<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.TreeTableView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.BorderPane?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>
<?import javafx.scene.text.Text?>

<BorderPane xmlns="http://javafx.com/javafx/8.0.171"
	xmlns:fx="http://javafx.com/fxml/1" fx:controller="BasketController">
   <center>
      <VBox alignment="TOP_CENTER" BorderPane.alignment="CENTER">
         <children>
            <TreeTableView />
         </children>
      </VBox>
   </center>
   <top>
      <BorderPane BorderPane.alignment="CENTER">
         <center>
            <Text strokeType="OUTSIDE" strokeWidth="0.0" text="OAXACO" BorderPane.alignment="CENTER">
               <font>
                  <Font name="Elephant" size="43.0" />
               </font>
            </Text>
         </center>
         <bottom>
            <BorderPane BorderPane.alignment="TOP_CENTER">
               <center>
                  <Text strokeType="OUTSIDE" strokeWidth="0.0" text="Mexican Restaurant">
                     <font>
                        <Font name="System Bold Italic" size="15.0" />
                     </font>
                     <BorderPane.margin>
                        <Insets left="5.0" right="5.0" />
                     </BorderPane.margin>
                  </Text>
               </center>
               <right>
                  <Button mnemonicParsing="false" text="Staff Login" BorderPane.alignment="CENTER" />
               </right>
               <left>
                  <Button mnemonicParsing="false" text="Call A Server" BorderPane.alignment="CENTER" />
               </left>
            </BorderPane>
         </bottom>
         <padding>
            <Insets bottom="5.0" left="5.0" right="5.0" top="5.0" />
         </padding>
      </BorderPane>
   </top>
   <bottom>
      <AnchorPane BorderPane.alignment="CENTER">
         <children>
            <Button layoutX="107.0" layoutY="53.0" mnemonicParsing="false" text="Confirm Order" />
            <Button layoutX="104.0" layoutY="81.0" mnemonicParsing="false" onMouseClicked="#returnToMenu" text="Return to Menu" />
         </children>
         <BorderPane.margin>
            <Insets bottom="10.0" />
         </BorderPane.margin>
      </AnchorPane>
   </bottom>
</BorderPane>
